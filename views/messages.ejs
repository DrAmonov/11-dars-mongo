<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <style>
    .box{
        background-color: aqua;
        border-radius: 12px;
        width: 250px;
        height: 200px;
        text-align: center;
    }
  </style>
  <body>
    <div>
      <%messages.forEach(el => { %>
      <div class="box">
        <div>
          <h5><%= el.username%></h5>
          <h5><%= el.email%></h5>
          <h5><%= el.phone%></h5>
          <h5><%= el.comment%></h5>
        </div>
      </div>
      <%}) %>
    </div>
  </body>
</html>
